{
  "name": "@manyeya/spane",
  "version": "0.2.0",
  "description": "Parallel Asynchronous Node Execution - Workflow orchestration engine built on BullMQ and Redis",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:all": "bun run build && bun run build:types",
    "prepublishOnly": "bun run build:all",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "workflow",
    "orchestration",
    "bullmq",
    "dag",
    "node-execution",
    "redis",
    "async"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manyeya/spane.git"
  },
  "bugs": {
    "url": "https://github.com/manyeya/spane/issues"
  },
  "homepage": "https://github.com/manyeya/spane#readme",
  "dependencies": {
    "bullmq": "^5.67.1",
    "dagre": "^0.8.5",
    "drizzle-orm": "^0.44.7",
    "ioredis": "^5.9.2",
    "jsonata": "^2.1.0",
    "lru-cache": "^11.2.5",
    "opossum": "^8.5.0",
    "pino": "^10.3.0",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dagre": "^0.7.53",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "fast-check": "^4.5.3",
    "pino-pretty": "^13.1.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
